{
  "name": "website",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:desktop",
    "dev:web": "vite --mode web",
    "dev:desktop": "vite --mode desktop",
    "build": "npm run build:desktop",
    "build:web": "vite build --mode web",
    "build:desktop": "vite build --mode desktop && npm run --workspace=desktop build"
  },
  "dependencies": {
    "element-plus": "^2.13.0",
    "lodash-es": "^4.17.22",
    "pinia": "^3.0.4",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "less": "^4.5.1",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.1"
  }
}
