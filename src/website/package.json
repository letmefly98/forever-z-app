{
  "name": "website",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run dev:desktop",
    "dev:web": "cross-env NODE_ENV=development vite --mode web",
    "dev:desktop": "cross-env NODE_ENV=development vite --mode desktop",
    "build": "pnpm run build:desktop",
    "build:web": "vite build --mode web",
    "build:desktop": "vite build --mode desktop && pnpm run --filter desktop build"
  },
  "dependencies": {
    "element-plus": "^2.8.8",
    "lodash-es": "^4.17.21",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.0",
    "common": "workspace:^",
    "cross-env": "7.0.3",
    "desktop": "workspace:^",
    "less": "^4.2.0",
    "types": "workspace:^",
    "typescript": "^5.7.2",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.11"
  }
}
